{
	"dockerComposeFile": [
		"docker-compose.yml"
	],
    "forwardPorts": [8081],
	"service": "development",
    "workspaceFolder": "/workspace/",
    "postCreateCommand": ".devcontainer/scripts/postCreateCommand.sh",
    "postStartCommand": ".devcontainer/scripts/postStartCommand.sh",
    "customizations": {
        "vscode": {
            "settings": {
                "files.associations": {
                    "*.rockspec" : "lua"
                }
            },
            "extensions": [
                "github.vscode-github-actions"
            ]
        }
    }
}